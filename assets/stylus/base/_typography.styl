/* Tipografia */

/* della-respira-regular - latin */
@font-face {
  font-family: 'Della Respira';
  font-style: normal;
  font-weight: 400;
  src: url('../fonts/della-respira/della-respira-v8-latin-regular.eot'); /* IE9 Compat Modes */
  src: local(''),
       url('../fonts/della-respira/della-respira-v8-latin-regular.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
       url('../fonts/della-respira/della-respira-v8-latin-regular.woff2') format('woff2'), /* Super Modern Browsers */
       url('../fonts/della-respira/della-respira-v8-latin-regular.woff') format('woff'), /* Modern Browsers */
       url('../fonts/della-respira/della-respira-v8-latin-regular.ttf') format('truetype'), /* Safari, Android, iOS */
       url('../fonts/della-respira/della-respira-v8-latin-regular.svg#DellaRespira') format('svg'); /* Legacy iOS */
}

